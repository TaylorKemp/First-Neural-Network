<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.stage.Screen?>
<?scenebuilder-stylesheet background.css?>
<?import java.net.URL?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane styleClass="background" maxHeight="400" maxWidth="600" minHeight="400" minWidth="600" stylesheets="@../../deeplearning2/pkg1/background.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="deeplearning2.pkg1.HomeController">
<GridPane styleClass="overlay" prefHeight="${screen.visualBounds.height}" prefWidth="${screen.visualBounds.width}" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0">
    <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="${screen.visualBounds.width}" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="${screen.visualBounds.width}" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="${screen.visualBounds.width}" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="500.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" maxHeight="100.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button fx:id="helpButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Help" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="BOTTOM" />
      <Button fx:id="loadButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Load Network" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
      <Button fx:id="newButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Create Network" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP" />
      <Label fx:id="title" text="Neural Network " GridPane.columnIndex="1" GridPane.halignment="CENTER" styleClass="title" />
   </children>
</GridPane>
<stylesheets><URL value="@background.css"/></stylesheets>
<fx:define><Screen fx:factory="getPrimary" fx:id="screen"/></fx:define>
</AnchorPane>
